{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "projects/easy-video-468917/locations/us-central1/operations/OPERATION_ID"
      },
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "app",
    "lib",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
